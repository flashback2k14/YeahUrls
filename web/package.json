{
  "name": "yeah-urls-web",
  "version": "1.1.0",
  "author": "flashback2k14",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf dist",
    "clean:n": "rm -rf node_modules",
    "start": "ng serve",
    "start:p": "ng serve -prod --named-chunks --port 4202",
    "start:o": "ng serve --aot --build-optimizer --named-chunks --port 4201",
    "build": "ng build",
    "build:p": "ng build -prod --named-chunks",
    "build:o": "ng build -prod --aot --build-optimizer --named-chunks --vendor-chunk --common-chunk",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "create:200": "cat dist/index.html >> dist/200.html",
    "copy:m": "cp -rv src/manifest.json dist/",
    "deploy": "npm run build:o && npm run copy:m && cd dist && npm run create:200 && surge --domain https://yeah-urls.surge.sh && npm run clean"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~5.0.2",
    "@angular/common": "~5.0.2",
    "@angular/compiler": "~5.0.2",
    "@angular/core": "~5.0.2",
    "@angular/forms": "~5.0.2",
    "@angular/http": "~5.0.2",
    "@angular/platform-browser": "~5.0.2",
    "@angular/platform-browser-dynamic": "~5.0.2",
    "@angular/router": "~5.0.2",
    "angular2-virtual-scroll": "~0.3.1",
    "core-js": "~2.5.1",
    "rxjs": "~5.5.2",
    "socket.io-client": "~2.0.4",
    "zone.js": "~0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "~1.5.2",
    "@angular/compiler-cli": "~5.0.2",
    "@angular/language-service": "~5.0.2",
    "@types/jasmine": "~2.8.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.0.53",
    "codelyzer": "~4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "surge": "~0.19.0",
    "protractor": "~5.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.1"
  }
}
