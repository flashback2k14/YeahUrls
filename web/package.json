{
  "name": "yeah-urls-web",
  "version": "1.4.0",
  "author": "flashback2k14",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf dist",
    "clean:n": "rm -rf node_modules",
    "start": "ng serve --aot",
    "start:o": "ng serve --aot --build-optimizer --named-chunks --port 4201",
    "start:p": "ng serve --prod --named-chunks --port 4202",
    "start:d": "ng serve --aot --host 0.0.0.0 --disable-host-check --port 4203",
    "serve:dist": "http-server -p 8080 -c-1 dist/",
    "build": "ng build",
    "build:p": "ng build --prod --named-chunks",
    "build:o": "ng build --prod --aot --build-optimizer --named-chunks --vendor-chunk --common-chunk",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "create:200": "cat dist/index.html >> dist/200.html",
    "deploy": "npm run clean && npm run build:o && cd dist && npm run create:200 && surge --domain https://yeah-urls.surge.sh && npm run clean"
  },
  "engines": {
    "node": "10.14.2",
    "npm": "6.4.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.4",
    "@angular/common": "~7.2.4",
    "@angular/compiler": "~7.2.4",
    "@angular/core": "~7.2.4",
    "@angular/forms": "~7.2.4",
    "@angular/http": "~7.2.4",
    "@angular/platform-browser": "~7.2.4",
    "@angular/platform-browser-dynamic": "~7.2.4",
    "@angular/pwa": "^0.13.1",
    "@angular/router": "~7.2.4",
    "@angular/service-worker": "~7.2.4",
    "core-js": "~2.6.5",
    "ngx-virtual-scroller": "~1.0.16",
    "rxjs": "~6.4.0",
    "socket.io-client": "~2.2.0",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.4",
    "@angular/language-service": "~7.2.4",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.9.4",
    "codelyzer": "~4.5.0",
    "http-server": "~0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "protractor": "~5.4.2",
    "surge": "~0.20.1",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "~3.3.3"
  }
}
