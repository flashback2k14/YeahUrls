{
  "name": "yeah-urls-web",
  "version": "1.2.0",
  "author": "flashback2k14",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf dist",
    "clean:n": "rm -rf node_modules",
    "start": "ng serve --aot",
    "start:p": "ng serve -prod --named-chunks --port 4202",
    "start:o": "ng serve --aot --build-optimizer --named-chunks --port 4201",
    "build": "ng build",
    "build:p": "ng build -prod --named-chunks",
    "build:o": "ng build -prod --aot --build-optimizer --named-chunks --vendor-chunk --common-chunk",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "create:200": "cat dist/index.html >> dist/200.html",
    "copy:m": "cp -rv src/manifest.json dist/",
    "deploy": "npm run build:o && npm run copy:m && cd dist && npm run create:200 && surge --domain https://yeah-urls.surge.sh _ && npm run clean"
  },
  "engines": {
    "node": "10.14.2",
    "npm": "6.4.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.3",
    "@angular/common": "~7.1.3",
    "@angular/compiler": "~7.1.3",
    "@angular/core": "~7.1.3",
    "@angular/forms": "~7.1.3",
    "@angular/http": "~7.1.3",
    "@angular/platform-browser": "~7.1.3",
    "@angular/platform-browser-dynamic": "~7.1.3",
    "@angular/router": "~7.1.3",
    "angular2-virtual-scroll": "~0.3.1",
    "core-js": "~2.6.1",
    "rxjs": "~6.3.3",
    "socket.io-client": "~2.2.0",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.3",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.3",
    "@angular/language-service": "~7.1.3",
    "@types/jasmine": "~3.3.2",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.15",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "surge": "~0.20.1",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.0",
    "typescript": "~3.1.1"
  }
}
